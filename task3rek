using System;

namespace fibonachirek
{
    class Program
    {
        static int fibonachirek(int n)
        {
            if (n == 0)
                return 0;
            if (n == 1)
                return 1;
            else return fibonachirek(n - 1) + fibonachirek(n - 2);
        }
        public static void Main(string[] args)
        {
            Console.WriteLine("введите i-ое число Фибоначи");
            int x = int.Parse(Console.ReadLine());
            Console.WriteLine(fibonachirek(x));
        }
    }
}